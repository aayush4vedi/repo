<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.swifiic.android.app.suta"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
    <permission android:name="swifiic.suta.USER" ></permission>

    <uses-permission android:name="de.tubs.ibr.dtn.permission.DTN_COMMUNICATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="swifiic.suta.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="swifiic.suta.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="swifiic.suta.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="swifiic.suta.MainActivity" />
        </activity>
        
        <provider
            android:name="swifiic.suta.provider.Provider"
            android:authorities="swifiic.suta"
            android:exported="true"
            android:permission="swifiic.suta.USER" >
        </provider>
     
    </application>

</manifest>
